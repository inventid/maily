{
  "name": "maily",
  "version": "1.1.0",
  "description": "Use standard React to create the HTML emails of your imagination without a headache",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dist && node dist/index.js",
    "dist": "babel src --out-dir dist --presets es2015,stage-2,react --plugins syntax-decorators",
    "watch": "babel src --out-dir dist -w --presets es2015,stage-2,react --plugins syntax-decorators",
    "prepublish": "npm run dist"
  },
  "author": "Joost Verdoorn & Steffan Sluis",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "htmlstrip-native": "^0.1.3",
    "mjml": "^2.0.2",
    "react": "^15.0.1"
  },
  "devDependencies": {
    "babel-cli": "~6.2.0",
    "babel-core": "~6.2.1",
    "babel-plugin-syntax-decorators": "^6.5.0",
    "babel-plugin-transform-runtime": "~6.1.18",
    "babel-preset-es2015": "~6.1.18",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "~6.1.18"
  }
}
