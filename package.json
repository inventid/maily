{
  "name": "maily",
  "version": "4.2.0-2",
  "description": "Use standard React to create the HTML emails of your imagination without a headache",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dist && node dist/index.js",
    "dist": "babel src --out-dir dist --presets es2015,stage-2,react,babel-polyfill --plugins syntax-decorators",
    "example": "babel . --ignore node_modules,dist,example-dist --out-dir example-dist --presets es2015,stage-2,react,babel-polyfill --plugins syntax-decorators && node example-dist/example.js",
    "watch": "babel src --out-dir dist -w --presets es2015,stage-2,react,babel-polyfill --plugins syntax-decorators",
    "prepublish": "npm run dist"
  },
  "author": "Joost Verdoorn & Steffan Sluis",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "html-minifier": "^3.5.21",
    "mjml": "4.2.0-beta.3",
    "pretty": "^2.0.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "striptags": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-core": "~6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "~6.18.0",
    "eslint-plugin-react": "^6.9.0"
  }
}
